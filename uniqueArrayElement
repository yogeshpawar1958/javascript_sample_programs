let arr=[1,2,5,2,1,8,3,2,1];
let b=[];

for(let i=0;i<arr.length;i++){
    if(b.indexOf(arr[i])===-1){
        b.push(arr[i]);
    }
}
console.log(b)//1,2,5,8,3


