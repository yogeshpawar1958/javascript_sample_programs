var val=97
for(let i=1;i<=5;i++){
    let str=""
    for(j=1;j<=i;j++){
       str+=`${String.fromCharCode(val)} `
       val+=1
    }
    console.log(str)
}

o/p===>
a 
b c 
d e f 
g h i j 
k l m n o



